{
  "name": "Reload plugin",
  "description": "Without an interaction reload your website",
  "version": "0.1",
  "manifest_version": 2,


  "browser_action": {
	"default_popup": "pop.html"

  },
  "background": {
    "js": ["reload.js"]
  },

  "permissions": [
	"activeTab",
  "activeTab",
  "storage"
  ]
}
